<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>
			Language labels for field labels.
			The file »locallang-form.xml« will be loaded from the path set as
			»settings.languageRootPath« in TypoScript.

			Localisation keys / label indexes follow the convention:
				* field.fieldID for field labels
		</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="field.typ">Type</label>
			<label index="field.name">Name</label>
		</languageKey>

		<languageKey index="de" type="array">
			<label index="field.typ">Typ</label>
			<label index="field.name">Name</label>
			<label index="field.title">Titel</label>
			<label index="field.author">Urheber</label>
			<label index="field.author.plural">Urheber</label>
			<label index="field.publisher">Verlag</label>
			<label index="field.publisher.plural">Verlage</label>
			<label index="field.publishDateSort">Erscheinungsdatum</label>
			<label index="field.series2">Enthalten in</label>
			<label index="field.series2.plural">Enthalten in</label>
			<label index="field.isbn">ISBN</label>
			<label index="field.isbn.plural">ISBN</label>
			<label index="field.size">Format</label>
			<label index="field.rvk_facet">Klassifikation</label>
			<label index="field.rvk_facet.plural">Klassifikation</label>
			<label index="field.contents">Beschreibung</label>
			<label index="field.language">Sprache</label>
			<label index="field.language.plural">Sprache</label>
		</languageKey>
	</data>
</T3locallang>
